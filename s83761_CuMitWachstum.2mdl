"Kupferpreis - Veränderungsfaktor"=
	IF THEN ELSE(Time <= 2015, "Kupferpreise (Lookup)"(Time)/"Kupferpreise (Lookup)"(Time\
		-1) , "Kupferpreis nach 2015 (Relativ)")
	~	
	~		|

Nachfrage= INTEG (
	((1 - "Kupferpreis - Veränderungsfaktor" + Wirtschaftswachstum) * Nachfrage),
		100)
	~	
	~		|

Veränderung in Prozent=
	(1-"Kupferpreis - Veränderungsfaktor")
	~	
	~		|

"Kupferpreis nach 2015 (Relativ)"=
	IF THEN ELSE(Nachfrage > Angebot, RANDOM NORMAL(1, 3 , 1.5 , 0.49 , 0), RANDOM NORMAL\
		(0.5, 1 , 0.75 , 0.2 , 0))
	~	
	~		|

Abbaukapazität=
	DELAY FIXED(Angebot * (Veränderung in Prozent * 1/2 + Wirtschaftswachstum), Bauzeit\
		, 0)
	~	
	~		|

Ausbaukapazität=
	DELAY FIXED(Angebot * (Veränderung in Prozent + Wirtschaftswachstum), Bauzeit, 0)
	~	
	~		|

Wirtschaftswachstum=
	0.02
	~	
	~		|

Angebot= INTEG (
	Kapazitätserweiterung,
		100)
	~	
	~		|

Kapazitätserweiterung=
	IF THEN ELSE( Nachfrage > Angebot, Ausbaukapazität, Abbaukapazität)
	~	
	~		|

Bauzeit=
	2
	~	
	~		|

