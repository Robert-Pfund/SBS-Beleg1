{UTF-8}
Veränderung in Prozent=
	IF THEN ELSE(Time > 2015, 1-Kupferpreis nach 2015, (1-"Kupferpreis - Veränderungsfaktor"\
		))
	~	
	~		|

Kupferpreis nach 2015=
	IF THEN ELSE( Nachfrage > Angebot, RANDOM NORMAL(0.01, 1.5, 1.25 , 0.25 , 0 ), RANDOM NORMAL\
		(-1.5, -0.01, -0.75 , 0.25 , 0 ))
	~	
	~		|

Nachfrage= INTEG (
	IF THEN ELSE(Time > 2015, (Kupferpreis nach 2015 /100 + Wirtschaftswachstum) * Nachfrage\
		, (1 - "Kupferpreis - Veränderungsfaktor"+Wirtschaftswachstum) * Nachfrage),
		100)
	~	
	~		|

Abbaukapazität=
	DELAY FIXED(Angebot * (Veränderung in Prozent * 1/2 + Wirtschaftswachstum), Bauzeit\
		, 0)
	~	
	~		|

Ausbaukapazität=
	DELAY FIXED(Angebot * (Veränderung in Prozent + Wirtschaftswachstum), Bauzeit, 0)
	~	
	~		|

Wirtschaftswachstum=
	0.02
	~	
	~		|

Angebot= INTEG (
	Kapazitätserweiterung,
		100)
	~	
	~		|

Kapazitätserweiterung=
	IF THEN ELSE( Nachfrage > Angebot, Ausbaukapazität, Abbaukapazität)
	~	
	~		|

Bauzeit=
	2
	~	
	~		|

"Kupferpreis - Veränderungsfaktor"=
	"Kupferpreise (Lookup)"(Time)/"Kupferpreise (Lookup)"(Time-1)
	~	
	~		|

"Kupferpreise (Lookup)"(
	[(0,0)-(3000,3000)],(1989,106),(1990,116.85),(1991,98),(1992,104),(1993,83),(1994,139\
		),(1995,121),(1996,100),(1997,78),(1998,67),(1999,86),(2000,84),(2001,66),(2002,70)\
