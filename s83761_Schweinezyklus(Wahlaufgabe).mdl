{UTF-8}
"(realer) Preis"= INTEG (
	(Nachfrage/"Kupferproduktion (Angebot)")*"(realer) Preis",
		5)
	~	
	~		|

"Kapazität-Abbaurate"=
	0.75
	~	
	~		|

"Kapazität-Aufbaurate"=
	1.25
	~	
	~		|

"Kupferproduktion (Angebot)"= INTEG (
	IF THEN ELSE(Nachfrage/"Kupferproduktion (Angebot)", "Kupferproduktion (Angebot)"*"Kapazität-Aufbaurate"\
		, "Kupferproduktion (Angebot)"*"Kapazität-Abbaurate"),
		40)
	~	
	~		|

Nachfrage= INTEG (
	IF THEN ELSE( (Preiserwartung in EUR > "(realer) Preis"), (Preiserwartung in EUR/"(realer) Preis"\
		)*Nachfrage, "(realer) Preis"/Preiserwartung in EUR*Nachfrage),
		60)
	~	
	~		|

Preiserwartung in EUR=
	DELAY FIXED("(realer) Preis", 2, 5.5)
	~	
	~		|

